{
	"name": "vscode-aem-sync",
	"displayName": "VSCode AEM Sync",
	"description": "Sync files, folders, nodes to AEM",
	"publisher": "yamato-ltd",
	"version": "1.0.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yamato-ltd/vscode-aem-sync.git"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/jcr_root/**"
	],
	"main": "dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.aempush",
				"title": "Export to AEM Server"
			},
			{
				"command": "extension.aempull",
				"title": "Import from AEM Server"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.aempush",
					"group": "AEM@100"
				},
				{
					"command": "extension.aempull",
					"group": "AEM@100"
				}
			],
			"explorer/context": [
				{
					"command": "extension.aempush",
					"group": "AEM@100"
				},
				{
					"command": "extension.aempull",
					"group": "AEM@100"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "AEM-Sync configuration",
			"properties": {
				"aemsync.server": {
					"type": "string",
					"default": "http://localhost:4502",
					"description": "Server URL"
				},
				"aemsync.user": {
					"type": "string",
					"default": "admin",
					"description": "Username"
				},
				"aemsync.password": {
					"type": "string",
					"default": "admin",
					"description": "Password"
				},
				"aemsync.autopush": {
					"type": "boolean",
					"default": false,
					"description": "Automitically synchronize file-system changes to server"
				},
				"aemsync.acceptSelfSignedCert": {
					"type": "boolean",
					"default": false,
					"description": "Accept self-signed certificates for HTTPs"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"dependencies": {
		"request": "~2.88.0",
		"adm-zip": "~>0.4.13",
		"q": "1.5.1",
		"glob": "3.2.9",
		"fs-extra": "8.1.0",
		"xmldom": "0.1.27",
		"xpath": "0.0.27",
		"unzip": "0.1.11",
		"archiver": "3.1.1"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.14",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.7.2",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.3",
		"vscode-test": "^1.2.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	}
}
